<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mdd.mt.mapper.MovieCinemaMapper">
    <resultMap id="BaseResultMap" type="com.mdd.mt.model.MovieCinema">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="moive_id" property="moiveId" jdbcType="INTEGER"/>
        <result column="cinema_id" property="cinemaId" jdbcType="INTEGER"/>
        <result column="create_time" property="createTime" jdbcType="DATE"/>
    </resultMap>

    <sql id="Base_Column_List">
    id, moive_id, cinema_id, create_time
  </sql>

    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select
        <include refid="Base_Column_List"/>
        from t_movie_cinema
        where id = #{id,jdbcType=INTEGER}
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
        delete from t_movie_cinema
        where id = #{id,jdbcType=INTEGER}
     </delete>

    <insert id="insert" parameterType="com.mdd.mt.model.MovieCinema">
        insert into t_movie_cinema (id, moive_id, cinema_id,
            create_time)
        values (#{id,jdbcType=INTEGER}, #{moiveId,jdbcType=INTEGER}, #{cinemaId,jdbcType=INTEGER},
            #{createTime,jdbcType=DATE})
    </insert>

    <!--批量插入-->
    <insert id="saveMovieCinemaList" parameterType="java.util.List">
        insert into t_movie_cinema (id, moive_id, cinema_id,
        create_time)
        VALUES
        <foreach collection="movieCinemaList" item="movie" index="index" separator=",">
            (#{id,jdbcType=INTEGER}, #{moiveId,jdbcType=INTEGER}, #{cinemaId,jdbcType=INTEGER},
            now())
        </foreach>
    </insert>

</mapper>